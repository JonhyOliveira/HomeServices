$TTL    604800
@       IN      SOA     ns.home. admin.home. (
                  4     ; Serial
             604800     ; Refresh
              86400     ; Retry
            2419200     ; Expire
             604800 )   ; Negative Cache TTL
;
; name servers - NS records
                   IN      NS      ns.home.      ; nameserver
;
; gateway
gateway.home.      IN      A      192.168.1.1
; name servers - A records
ns.home.           IN      A      192.168.1.2 
;
; 192.168.0.0/16 - A records
host1.home.        IN      A      192.168.1.3
host2.home.        IN      A      192.168.1.4
host3.home.        IN      A      192.168.1.5
